  <script>
	$(function(){
		window.onload = function()
		{
			var $li = $('#tab li');
			var $ul = $('#content ul');
			$li.mouseover(function(){
				var $this = $(this);
				var $t = $this.index();
				$li.removeClass();
				$this.addClass('act_active');
				$ul.css('display','none');
				$ul.eq($t).css('display','block');
			})
		}
	
	});  
</script>

